.section{
    padding:24rpx 30rpx;
}
.personInfo{
    margin-bottom: 60rpx;
}
.personInfo .text{
    margin-bottom: 16rpx;
    font-size: 34rpx;
    color:#000000;
    line-height: 51rpx;
}
.text-success{
    display: flex;
    padding: 10rpx 0 0 16rpx;
    background: #F6FFED;
    border: 1px solid #B7EB8F;
    border-radius: 4px;
    font-size: 28rpx;
    color:rgba(0,0,0,0.65);
}
.text-fail{
    display: flex;
    background: #FFF1F0;
    border: 1px solid #FFA39E;
    border-radius: 4px;
}
.tip-icon{
    padding:10rpx  16rpx;
}
.tip-text{
    font-size: 28rpx;
    color:rgba(0,0,0,0.65);
    line-height:58rpx;
}
.btnGroup{
    display: flex;
    justify-content: space-between;
}
.btnGroup .btn{
    width:310rpx;
    height:70rpx;
    line-height:70rpx;
    text-align: center;
    border-radius: 10px;
    font-size: 32rpx;
}
.btnGroup .check-fail{
    border:2rpx solid #F5222D;
    color:#F5222D;
}
.btnGroup .check-success{
    border:2rpx solid #1AAD19;
    color:#1AAD19;
}

