.container {
    position: relative;
}
/* tab列 */
#tab {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 111;
}
.tab {
    position: relative;
    width: 690rpx;
    padding: 11rpx 30rpx;
    background-color: white;
    border-bottom:1rpx solid #dfdfdf;
}

.tab-list {
    display: inline-block;
    margin-right:77rpx;
    font-size: 26rpx;
    color: rgba(0, 0, 0, 0.45);
    line-height:50rpx;
}
.search{
    display: inline-block;
    position: absolute;
    right:0;
    top:19rpx;
    width:100rpx;
    height: 40rpx;
    text-align: center;
    border-left:1rpx solid #E8E8E8;
}
.search-icon{
    width:40rpx;
    height:40rpx;
}
.arrow-icon{
    width:15rpx;
    height:10rpx;
    margin-left: 8rpx;
}
.on {
    color: #1890FF;
}
/* 筛选框 */
.filter{
     width: 100%;
     height: 1050rpx;
     background: #fff;
}
.shaixuankuang {
    width: 100%;   
    padding-left:30rpx;
    overflow-y: auto;
}
.shaixuan {
    width: 100%; 
    height: auto; 
    overflow: hidden; 
} 
.shuaixuantiaojian {
    display: block;
    padding: 32rpx 0 15rpx 0;
    font-size: 26rpx;
    color: rgba(0,0,0,0.85);
    line-height:50rpx;
}
.chose-txt {
    float: left;
    width: 331rpx;
    line-height:63rpx;
    margin: 0 28rpx 24rpx 0;
    text-align: center;
    background: #F5F5F5;
    border: 1rpx solid #F5F5F5;
    border-radius: 5rpx;
}
.chose-p {
    line-height:63rpx;
    font-size: 26rpx;
    color: #333333;
}
.chosed{
    background: #E6F7FF;
    border: 1rpx solid #91D5FF;
}
.payTime {
    float: left;
    width: 331rpx;
    height:63rpx;
    text-align: center;
    margin-right:28rpx;
    border: 1px solid rgba(0,0,0,0.15);
    border-radius: 4px;
}
/* 按钮组 */
.btn-group {
    width: 100%;
    display: flex;
    margin-top:10rpx;
}
.btn-group .reset{
    float: left; 
    width:50%;
    line-height:85rpx;
    background: #E9F3FC;
    font-size: 26rpx;
    color: #333333;
    text-align: center;
}
.btn-group .queren{
    float: left; 
    width:50%;
    line-height:85rpx;
    background-image: linear-gradient(201deg, #635DFF 0%, #1E8EFF 95%);
    font-size: 26rpx;
    color: #fff;
    text-align: center;
}
/* 支付方式和支付状态 */
.location_bottom {
    width: 100%;
    height: 410rpx;
    background: #fff;
    font-size: 34rpx;
    text-align: center; 
}
.add-all {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #E8E8E8;
    color: #333;
}
.add-one {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #1890FF;
    color: #1890FF;
}
/* 遮罩层 */
.swiper-beijing {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    width: 100%;
    height: 1200rpx;
    background: rgba(0,0,0,0.40);
}
/* 订单列表 */
.order-list {
    width:100%;
    padding:72rpx 0 0 30rpx;
}
.order-item{
    display: flex;
    padding:23rpx 0;
    border-bottom: 1rpx solid #eee;
}
.order-img{
    width:77rpx;
    height:77rpx;
    margin-right:24rpx;
}
.order-img image{
    width:100%;
    height: 100%;
}
.order-info{
    display: flex;
    flex-direction: column;
}
.base-info{
    display: flex;
    line-height:51rpx;
    font-size: 28rpx;
    color: #000;
}
.base-info .name{
    width:100rpx;
}
.base-info .subject{
    width:360rpx;
}
.base-info .cost{
    font-size: 36rpx;
}
.other-info{
    line-height:22rpx;
    font-size:20rpx;
    color:rgba(0,0,0,0.65);
}
.shown {
    display: block;
}
.hidden {
    display: none;
} 


